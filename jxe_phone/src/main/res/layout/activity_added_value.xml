<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/menu_background"
    android:orientation="vertical" >

    <TextView
        android:id="@+id/edt_added_title"
        android:layout_width="fill_parent"
        android:layout_height="30.0dip"
        android:background="@drawable/layout_menu_top"
        android:gravity="center"
        android:text="附加值录入"
        android:textColor="@color/black" />

    <ScrollView
        android:id="@+id/scrollView1"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10.0dip"
        android:layout_marginRight="10.0dip" >

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal" >

                <RadioGroup
                    android:id="@+id/radiogroup1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:orientation="horizontal" >

                    <RadioButton
                        android:id="@+id/rb_middle_driver"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="中巴长"
                        android:textColor="@color/black" />

                    <RadioButton
                        android:id="@+id/rb_small_driver"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="小巴长"
                        android:textColor="@color/black" />

                    <RadioButton
                        android:id="@+id/rb_passenger"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="巴员"
                        android:textColor="@color/black" />
                </RadioGroup>
            </LinearLayout>

            <TableLayout
                android:id="@+id/tableLayout1"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical" >

                <TableRow
                    android:id="@+id/tableRow15"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center" >

                    <TextView
                        android:id="@+id/textView22"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="所属巴组"
                        android:textColor="@color/black" />

                    <Spinner
                        android:id="@+id/spn_added_genusGroup"
                        android:layout_width="0.0dip"
                        android:layout_height="40.0dip"
                        android:layout_marginLeft="5.0dip"
                        android:layout_weight="1.0"
                        android:background="@drawable/spinner_click_selector"
                        android:gravity="center"
                        android:textColor="@color/black" />

                    <TextView
                        android:id="@+id/textView23"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="巴组人数"
                        android:textColor="@color/black" />

                    <Spinner
                        android:id="@+id/spn_added_groupNo"
                        android:layout_width="0.0dip"
                        android:layout_height="40.0dip"
                        android:layout_marginLeft="5.0dip"
                        android:layout_weight="1.0"
                        android:background="@drawable/spinner_click_selector"
                        android:gravity="center"
                        android:textColor="@color/black" />
                </TableRow>

                <TableRow
                    android:id="@+id/tableRow13"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="2.0dip"
                    android:gravity="center_vertical" >

                    <TextView
                        android:id="@+id/textView13"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="票数"
                        android:textColor="@color/black" />

                    <EditText
                        android:id="@+id/edt_added_pollCount"
                        android:layout_width="0.0dip"
                        android:layout_height="40.0dip"
                        android:layout_marginLeft="5.0dip"
                        android:layout_weight="1.0"
                        android:background="@drawable/edt_logininput_selector"
                        android:inputType="number" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="票数计提"
                        android:textColor="@color/black" />

                    <EditText
                        android:id="@+id/edt_added_pollCount_compute"
                        android:layout_width="0.0dip"
                        android:layout_height="40.0dip"
                        android:layout_marginLeft="5.0dip"
                        android:layout_weight="1.0"
                        android:background="@drawable/edt_input_normal"
                        android:inputType="text" />
                </TableRow>

                <TableRow
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="2.0dip"
                    android:gravity="center_vertical" >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="计费重量"
                        android:textColor="@color/black" />

                    <EditText
                        android:id="@+id/edt_added_money_weight"
                        android:layout_width="0.0dip"
                        android:layout_height="40.0dip"
                        android:layout_marginLeft="5.0dip"
                        android:layout_weight="1.0"
                        android:background="@drawable/edt_input_normal"
                        android:inputType="numberDecimal" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="计提重量"
                        android:textColor="@color/black" />

                    <EditText
                        android:id="@+id/edt_added_compute_weight"
                        android:layout_width="0.0dip"
                        android:layout_height="40.0dip"
                        android:layout_marginLeft="5.0dip"
                        android:layout_weight="1.0"
                        android:background="@drawable/edt_input_normal"
                        android:inputType="numberDecimal" />
                </TableRow>

                <TableRow
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="2.0dip"
                    android:gravity="center_vertical" >

                    <TextView
                        android:id="@+id/textView1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="重量计提标准"
                        android:textColor="@color/black" />

                    <EditText
                        android:id="@+id/edt_added_compute_weight_standard"
                        android:layout_width="0.0dip"
                        android:layout_height="40.0dip"
                        android:layout_marginLeft="5.0dip"
                        android:layout_weight="1.0"
                        android:background="@drawable/edt_logininput_selector" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="超区域派送费"
                        android:textColor="@color/black" />

                    <EditText
                        android:id="@+id/edt_added_exceed_region_money"
                        android:layout_width="0.0dip"
                        android:layout_height="40.0dip"
                        android:layout_marginLeft="5.0dip"
                        android:layout_weight="1.0"
                        android:background="@drawable/edt_logininput_selector" />
                </TableRow>

                <TableRow
                    android:id="@+id/tableRow1"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="2.0dip"
                    android:gravity="center_vertical" >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="代收款"
                        android:textColor="@color/black" />

                    <EditText
                        android:id="@+id/edt_added_goodsChargeFee"
                        android:layout_width="0.0dip"
                        android:layout_height="40.0dip"
                        android:layout_marginLeft="5.0dip"
                        android:layout_weight="2.0"
                        android:background="@drawable/edt_logininput_selector"
                        android:inputType="numberDecimal" />
                </TableRow>

                <TableRow
                    android:id="@+id/tableRow2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="2.0dip"
                    android:gravity="center_vertical" >

                    <TextView
                        android:id="@+id/textView2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="代收款计提"
                        android:textColor="@color/black" />

                    <EditText
                        android:id="@+id/edt_added_goodsChargeFee_compute"
                        android:layout_width="0.0dip"
                        android:layout_height="40.0dip"
                        android:layout_marginLeft="5.0dip"
                        android:layout_weight="1.0"
                        android:background="@drawable/edt_logininput_selector"
                        android:inputType="text"
                        android:singleLine="true" />
                </TableRow>

                <TableRow
                    android:id="@+id/tableRow3"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="2.0dip"
                    android:gravity="center_vertical" >

                    <TextView
                        android:id="@+id/textView3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="上门费"
                        android:textColor="@color/black" />

                    <EditText
                        android:id="@+id/edt_added_fuelServiceFee"
                        android:layout_width="0.0dip"
                        android:layout_height="40.0dip"
                        android:layout_marginLeft="5.0dip"
                        android:layout_weight="1.0"
                        android:background="@drawable/edt_logininput_selector"
                        android:inputType="numberDecimal" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="目标收入"
                        android:textColor="@color/black" />

                    <EditText
                        android:id="@+id/edt_added_targetIncome"
                        android:layout_width="0.0dip"
                        android:layout_height="40.0dip"
                        android:layout_marginLeft="5.0dip"
                        android:layout_weight="1.0"
                        android:background="@drawable/edt_logininput_selector"
                        android:inputType="numberDecimal" />
                </TableRow>

                <TableRow
                    android:id="@+id/tableRow4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="2.0dip"
                    android:gravity="center_vertical" >

                    <TextView
                        android:id="@+id/textView4"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="快递派送运费"
                        android:textColor="@color/black" />

                    <EditText
                        android:id="@+id/edt_added_express_delivery_freight"
                        android:layout_width="0.0dip"
                        android:layout_height="40.0dip"
                        android:layout_marginLeft="5.0dip"
                        android:layout_weight="1.0"
                        android:background="@drawable/edt_logininput_selector"
                        android:inputType="numberDecimal"
                        android:singleLine="true" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="快递派送计提标准"
                        android:textColor="@color/black" />

                    <EditText
                        android:id="@+id/edt_added_express_delivery_freight_standard"
                        android:layout_width="0.0dip"
                        android:layout_height="40.0dip"
                        android:layout_marginLeft="5.0dip"
                        android:layout_weight="1.0"
                        android:background="@drawable/edt_logininput_selector"
                        android:inputType="text"
                        android:singleLine="true" />
                </TableRow>

                <TableRow
                    android:id="@+id/tableRow5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5.0dip"
                    android:gravity="center_vertical" >

                    <TextView
                        android:id="@+id/textView5"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="外阜-外阜运费"
                        android:textColor="@color/black" />

                    <EditText
                        android:id="@+id/edt_added_outward"
                        android:layout_width="0.0dip"
                        android:layout_height="40.0dip"
                        android:layout_marginLeft="5.0dip"
                        android:layout_weight="1.0"
                        android:background="@drawable/edt_logininput_selector"
                        android:inputType="numberDecimal"
                        android:singleLine="true" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="外阜-外阜计提标准"
                        android:textColor="@color/black" />

                    <EditText
                        android:id="@+id/edt_added_outward_standard"
                        android:layout_width="0.0dip"
                        android:layout_height="40.0dip"
                        android:layout_marginLeft="5.0dip"
                        android:layout_weight="1.0"
                        android:background="@drawable/edt_logininput_selector"
                        android:inputType="text"
                        android:singleLine="true" />
                </TableRow>
            </TableLayout>

            <TableRow
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5.0dip"
                android:gravity="center_vertical" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="外阜-西安运费"
                    android:textColor="@color/black" />

                <EditText
                    android:id="@+id/edt_added_xian"
                    android:layout_width="0.0dip"
                    android:layout_height="40.0dip"
                    android:layout_marginLeft="5.0dip"
                    android:layout_weight="1.0"
                    android:background="@drawable/edt_logininput_selector"
                    android:inputType="number"
                    android:singleLine="true" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="外阜-西安计提标准"
                    android:textColor="@color/black" />

                <EditText
                    android:id="@+id/edt_added_xian_standard"
                    android:layout_width="0.0dip"
                    android:layout_height="40.0dip"
                    android:layout_marginLeft="5.0dip"
                    android:layout_weight="1.0"
                    android:background="@drawable/edt_logininput_selector"
                    android:inputType="text"
                    android:singleLine="true" />
            </TableRow>

            <TableLayout
                android:id="@+id/tableLayout2"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5.0dip"
                android:gravity="center_vertical" >

                <LinearLayout
                    android:id="@+id/tableRow6"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical" >

                    <TextView
                        android:id="@+id/textView6"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="固定费用"
                        android:textColor="@color/black" />

                    <EditText
                        android:id="@+id/edt_added_fixed_money"
                        android:layout_width="0.0dip"
                        android:layout_height="40.0dip"
                        android:layout_marginLeft="5.0dip"
                        android:layout_weight="1.0"
                        android:background="@drawable/edt_logininput_selector"
                        android:inputType="numberDecimal"
                        android:singleLine="true" />

                    <TextView
                        android:id="@+id/textView7"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="日常费用"
                        android:textColor="@color/black" />

                    <EditText
                        android:id="@+id/edt_added_daily_money"
                        android:layout_width="0.0dip"
                        android:layout_height="40.0dip"
                        android:layout_weight="1.0"
                        android:background="@drawable/edt_logininput_selector"
                        android:inputType="numberDecimal" />
                </LinearLayout>
            </TableLayout>
        </LinearLayout>
    </ScrollView>

    <LinearLayout
        android:id="@+id/linearLayout1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="10.0dip"
        android:gravity="center_vertical"
        android:orientation="horizontal" >

        <Button
            android:id="@+id/btn_added_cancel"
            android:layout_width="0.0dip"
            android:layout_height="wrap_content"
            android:layout_marginRight="5.0dip"
            android:layout_weight="1.0"
            android:background="@drawable/btn_selector"
            android:text="取消" />

        <Button
            android:id="@+id/btn_added_ok"
            android:layout_width="0.0dip"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5.0dip"
            android:layout_weight="1.0"
            android:background="@drawable/btn_selector"
            android:text="保存" />
    </LinearLayout>

</LinearLayout>