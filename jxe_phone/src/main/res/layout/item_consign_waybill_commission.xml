<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <LinearLayout
        android:id="@+id/lin_content"
        android:layout_width="match_parent"
        android:layout_height="@dimen/sendpieces_sign_table_height"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/item_txt_consignor_commission_code"
            android:layout_width="100dp"
            android:layout_height="@dimen/sendpieces_sign_table_height"
            android:layout_gravity="center"
            android:gravity="center"
            android:text="工号" />

        <View
            android:layout_width="1px"
            android:layout_height="@dimen/sendpieces_sign_table_height"
            android:background="#cccccc" />

        <TextView
            android:id="@+id/item_txt_consignor_commission_piaonum"
            android:layout_width="70dp"
            android:layout_height="@dimen/sendpieces_sign_table_height"
            android:layout_gravity="center"
            android:gravity="center"
            android:text="票数" />

        <View
            android:layout_width="1px"
            android:layout_height="@dimen/sendpieces_sign_table_height"
            android:background="#cccccc" />

        <TextView
            android:id="@+id/item_txt_consignor_commission_quantityynum"
            android:layout_width="150dp"
            android:layout_height="@dimen/sendpieces_sign_table_height"
            android:layout_gravity="center"
            android:gravity="center"
            android:text="件数" />

        <View
            android:layout_width="1px"
            android:layout_height="@dimen/sendpieces_sign_table_height"
            android:background="#cccccc" />

        <TextView
            android:id="@+id/item_txt_consignor_commission_expressFee"
            android:layout_width="150dp"
            android:layout_height="@dimen/sendpieces_sign_table_height"
            android:layout_gravity="center"
            android:gravity="center"
            android:text="运费" />
        
        <View
            android:layout_width="1px"
            android:layout_height="@dimen/sendpieces_sign_table_height"
            android:background="#cccccc" />

        <TextView
            android:id="@+id/item_txt_consignor_commission"
            android:layout_width="150dp"
            android:layout_height="@dimen/sendpieces_sign_table_height"
            android:layout_gravity="center"
            android:gravity="center"
            android:text="提成" />


        <View
            android:layout_width="1px"
            android:layout_height="@dimen/sendpieces_sign_table_height"
            android:background="#cccccc" />

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="#cccccc" />

</LinearLayout>